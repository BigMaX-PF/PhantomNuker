LoginButton.Name = "LoginButton"
LoginButton.Parent = LoginFrame
LoginButton.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
LoginButton.Position = UDim2.new(0.22133334, 0, 0.705215394, 0)
LoginButton.Size = UDim2.new(0, 205, 0, 50)
LoginButton.Font = Enum.Font.SourceSans
LoginButton.Text = "Login"
LoginButton.TextColor3 = Color3.fromRGB(255, 255, 255)
LoginButton.TextScaled = true
LoginButton.TextSize = 14.000
LoginButton.TextWrapped = true
LoginButton.MouseButton1Down:connect(function()
	if username.Text == "1Day" and password.Text == "GO" then
		LoginFrame.Visible = false
		PhantomNukerFrame.Visible = true
	end

end)

