PhantomNuker.Name = "Phantom Nuker"
PhantomNuker.Parent = game.CoreGui

LoginFrame.Name = "LoginFrame"
LoginFrame.Parent = PhantomNuker
LoginFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
LoginFrame.Position = UDim2.new(0.362516016, 0, 0.14419657, 0)
LoginFrame.Size = UDim2.new(0, 375, 0, 441)
LoginFrame.Active = true
LoginFrame.Draggable = true

